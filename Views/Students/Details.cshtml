@model LABA3.Models.Entities.Student

<h1>Course</h1>

<table class="table">
    <thead>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Age</th>
        <th>User Id</th>
        <th>College Id</th>
        <th>Courses</th>
    </tr>
    </thead>
    <tbody>
    <tr>
        <td>@Model.Id</td>
        <td>@Model.Name</td>
        <td>@Model.Age</td>
        <td>@Model.UserId</td>
        <td>@Model.CollegeId</td>
        <td>@foreach (var course in Model.Courses)
            {
                <li>@course.Name</li>
            }
        </td>
            
    </tr>
    </tbody>
</table>

<div>
    <a class="mt-3" asp-action="Index">Back to List</a>
</div>