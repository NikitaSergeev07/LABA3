@model LABA3.Models.Entities.Course

<h1>Course</h1>

<table class="table">
    <thead>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Students</th>
    </tr>
    </thead>
    <tbody>
    <tr>
        <td>@Model.Id</td>
        <td>@Model.Name</td>
        <td>@foreach (var student in Model.Students)
            {
                <li>@student.Name</li>
            }
        </td>
            
    </tr>
    </tbody>
</table>

<div>
    <a class="mt-3" asp-action="Index">Back to List</a>
</div>